{
  "name": "Share MicroContext using URLs or Links",
  "short_name": "MicroContext",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Share links to particular sections of content on a web page using Link or URL",
  "homepage_url": "http://learningpaths.io",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}